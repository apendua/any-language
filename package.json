{
  "name": "anti-language",
  "version": "0.1.0",
  "description": "A toolbelt for creating cool parsers and interpreters",
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-webpack": "webpack --progress",
    "clean": "rimraf lib coverage",
    "coverage": "babel-node $(npm bin)/isparta cover _mocha -- 'src/**/*.test.js' 'examples/**/*.test.js'",
    "coverage-report": "http-server ./coverage/lcov-report",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "mocha --compilers js:babel-core/register 'src/**/*.test.*'",
    "test-watch": "mocha --watch --compilers js:babel-core/register 'src/**/*.test.*' 'examples/**/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apendua/any-language.git"
  },
  "bugs": {
    "url": "https://github.com/apendua/any-language/issues"
  },
  "homepage": "https://github.com/apendua/any-language",
  "keywords": [
    "pratt",
    "parser",
    "grammar",
    "interpreter",
    "tokenizer"
  ],
  "author": "apendua",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.11.6",
    "chai": "^4.0.2",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "isparta": "^4.0.0",
    "jsverify": "^0.8.2",
    "live-server": "^1.0.0",
    "lodash.sample": "^4.2.0",
    "mocha": "^3.4.2",
    "npm-watch": "^0.1.5",
    "shortid": "^2.2.6",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0"
  }
}
